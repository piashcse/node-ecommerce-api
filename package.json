{
  "name": "nodeapi",
  "version": "1.0.0",
  "description": "Node.js API with Express, TypeORM, and PostgreSQL",
  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "build": "tsc",
    "lint": "eslint \"**/*.{js,mjs,cjs,ts}\"",
    "lint:fix": "eslint \"**/*.{js,mjs,cjs,ts}\" --fix",
    "format": "prettier --write \"**/*.{js,mjs,cjs,ts,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,mjs,cjs,ts,json,css,md}\""
  },
  "keywords": [
    "node",
    "express",
    "typeorm",
    "postgresql",
    "api"
  ],
  "author": "Piash",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^16.11.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4",
    "typescript-eslint": "^8.15.0"
  },
  "engines": {
    "node": ">=18"
  }
}
